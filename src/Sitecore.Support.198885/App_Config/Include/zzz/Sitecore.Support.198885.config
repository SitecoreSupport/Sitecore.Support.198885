<configuration xmlns:patch="http://www.sitecore.net/xmlconfig/" xmlns:set="http://www.sitecore.net/xmlconfig/set/" xmlns:role="http://www.sitecore.net/xmlconfig/role/">
  <sitecore role:require="Standalone OR ContentManagement OR ContentDelivery OR Processing OR Reporting">
    <pipelines>
      <processMessage>
        <processor type="Sitecore.Forms.Core.Pipelines.ProcessMessage, Sitecore.Forms.Core" set:type="Sitecore.Support.Forms.Core.Pipelines.ProcessMessage, Sitecore.Support.198885" method="ExpandTokens"></processor>
      </processMessage>
    </pipelines>
  </sitecore>
</configuration>